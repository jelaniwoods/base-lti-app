<tr>
  <td>
    <%= credential.consumer_key %>
  </td>

  <td>
    <%= credential.consumer_secret %>
  </td>

  <td>
    <%= credential.administrator_id %>
  </td>

  <td>
    <%= credential.enabled %>
  </td>

  <td>
    <div class="btn-group">
      <%= link_to credential, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-search-plus fa-fw"></i>
      <% end %>

      <%= link_to edit_credential_url(credential), class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-edit fa-fw"></i>
      <% end %>

      <%= link_to credential, method: :delete, data: { confirm: "Do you really want to delete this credential?" }, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-trash-alt fa-fw"></i>
      <% end %>
    </div>
  </td>
</tr>
