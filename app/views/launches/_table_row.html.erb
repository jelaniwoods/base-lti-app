<tr>
  <td>
    <%= launch.context_id %>
  </td>

  <td>
    <%= launch.resource_id %>
  </td>

  <td>
    <%= launch.enrollment_id %>
  </td>

  <td>
    <%= launch.user_id %>
  </td>

  <td>
    <%= launch.tool_consumer_id %>
  </td>

  <td>
    <%= launch.payload %>
  </td>

  <td>
    <div class="btn-group">
      <%= link_to launch, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-search-plus fa-fw"></i>
      <% end %>

      <%= link_to edit_launch_url(launch), class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-edit fa-fw"></i>
      <% end %>

      <%= link_to launch, method: :delete, data: { confirm: "Do you really want to delete this launch?" }, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-trash-alt fa-fw"></i>
      <% end %>
    </div>
  </td>
</tr>
