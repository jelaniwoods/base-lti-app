<tr>
  <td>
    <%= tool_consumer.instance_guid %>
  </td>

  <td>
    <%= tool_consumer.instance_name %>
  </td>

  <td>
    <%= tool_consumer.instance_description %>
  </td>

  <td>
    <%= tool_consumer.instance_url %>
  </td>

  <td>
    <%= tool_consumer.instance_contact_email %>
  </td>

  <td>
    <div class="btn-group">
      <%= link_to tool_consumer, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-search-plus fa-fw"></i>
      <% end %>

      <%= link_to edit_tool_consumer_url(tool_consumer), class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-edit fa-fw"></i>
      <% end %>

      <%= link_to tool_consumer, method: :delete, data: { confirm: "Do you really want to delete this tool_consumer?" }, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-trash-alt fa-fw"></i>
      <% end %>
    </div>
  </td>
</tr>
